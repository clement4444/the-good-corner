{
  "scripts": {
    "dev": "concurrently -c green,yellow -t \"HH:mm:ss\" -p \"{name} {time}\" \"npm:dev:*\"",
    "dev:client": "npm run dev --workspace=frontend",
    "dev:serveur": "npm run dev --workspace=backend",
    "codegen": "npm run codegen --workspace=frontend"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "workspaces": [
    "frontend",
    "backend"
  ]
}